{
  "name": "project-capture-ai",
  "version": "1.0.0",
  "description": "A web application for electrical project managers to capture and organize project notes, photos, and videos with AI-powered organization",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "setup-check": "node setup-check.js",
    "client": "cd client && npm start"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "googleapis": "^126.0.1",
    "dotenv": "^16.3.1",
    "@google/generative-ai": "^0.2.1",
    "express-fileupload": "^1.4.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.x"
  },
  "keywords": [
    "project-management",
    "google-drive",
    "ai",
    "file-upload",
    "electrical-projects"
  ],
  "author": "Project Capture AI",
  "license": "MIT"
}